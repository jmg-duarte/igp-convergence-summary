@article{Callon1990,
abstract = {This memo specifies an integrated routing protocol, based on$\backslash$nthe OSI Intra-Domain IS-IS Routing Protocol, which may be used as an$\backslash$ninterior gateway protocol (IGP) to support TCP/IP as well as OSI. This$\backslash$nallows a single routing protocol to be used to support pure IP$\backslash$nenvironments, pure OSI environments, and dual environments. This$\backslash$nspecification was developed by the IS-IS working group of the Internet$\backslash$nEngineering Task Force.},
author = {Callon, R},
doi = {10.17487/rfc1195},
issn = {2070-1721},
journal = {https://tools.ietf.org/html/rfc1195},
number = {1195},
pages = {85},
title = {{rfc-1195: Use of OSI IS-IS for Routing in TCP/IP and Dual Environments}},
url = {https://tools.ietf.org/html/rfc1195 http://www.rfc-editor.org/rfc/rfc1195.txt},
year = {1990}
}
